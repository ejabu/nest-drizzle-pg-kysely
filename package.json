{
  "name": "nest-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "dbml": "node --experimental-specifier-resolution=node --loader ts-node/esm docs/dbml.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --passWithNoTests --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "db:migrate": "rm -rf build-migration && tsc -p tsconfig-db.json && sequelize db:migrate --env local",
    "db:migrate:undo": "tsc -p tsconfig-db.json && sequelize db:migrate:undo",
    "db:migrate:undo:all": "tsc -p tsconfig-db.json && sequelize db:migrate:undo:all",
    "db:seed:all": "rm -rf build-migration && tsc -p tsconfig-db.json && sequelize db:seed:all",
    "db:seed": "rm -rf build-migration && tsc -p tsconfig-db.json && sequelize db:seed",
    "db:seed:undo": "tsc -p tsconfig-db.json && sequelize db:seed:undo",
    "db:seed:undo:all": "tsc -p tsconfig-db.json && sequelize db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "@nestjs/bullmq": "^10.1.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.2.6",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/terminus": "^8.1.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/jest": "27.0.2",
    "@types/pg-cursor": "^2.7.2",
    "@types/pg-query-stream": "^3.4.0",
    "@types/uuid": "^9.0.8",
    "aws-sdk": "^2.1549.0",
    "body-parser": "^1.20.2",
    "bullmq": "^5.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.31.2",
    "generic-pool": "^3.9.0",
    "husky": "^9.0.10",
    "integration-processing-service": "link:",
    "jest": "^27.2.5",
    "jest-cli": "^27.5.1",
    "jwt-decode": "^4.0.0",
    "kysely": "^0.27.3",
    "nats": "^2.19.0",
    "nest": "^0.1.6",
    "passport": "^0.5.2",
    "pg": "^8.11.3",
    "pg-cursor": "^2.10.3",
    "pg-query-stream": "^4.5.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sequelize": "6.35.2",
    "sequelize-typescript": "^2.1.6",
    "ts-jest": "^27.0.3",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "5.5.1-rc",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/node": "^20.11.13",
    "@types/validator": "^13.11.8",
    "drizzle-dbml-generator": "^0.7.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^2.0.20",
    "prettier": "3.1.0",
    "sequelize-cli": "6.6.2",
    "ts-node": "^10.9.2"
  }
}
